<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Data Tables</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="../../Content/assets3/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="../../Content/assets3/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="../../Content/assets3/bower_components/Ionicons/css/ionicons.min.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="../../Content/assets3/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css">
    <!-- Morris charts -->
    <link rel="stylesheet" href="../../Content/assets3/bower_components/morris.js/morris.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="../../Content/assets3/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="../../Content/assets3/dist/css/skins/_all-skins.min.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-red sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="~/Home/Index" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>Am</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Amendis </b><small>veolia</small></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        
                        <!-- Notifications: style can be found in dropdown.less -->
                        
                        <!-- Tasks: style can be found in dropdown.less -->
                        
                        <!-- User Account: style can be found in dropdown.less -->
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="dropdown user user-menu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <img src="../../Content/assets3/dist/img/user2-160x160.jpg" class="user-image" alt="User Image">
                                    <span class="hidden-xs">@User.Identity.Name</span>
                                </a>
                                <ul class="dropdown-menu">
                                    <!-- User image -->
                                    <li class="user-header">
                                        <img src="../../Content/assets3/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                                        <p>
                                            @User.Identity.Name
                                            @if (User.IsInRole("Admin"))
                                            {<small>Administrateur</small>}
                                            @if (User.IsInRole("Reception employee"))
                                            {<small>Employé de réception</small>}
                                            @if (User.IsInRole("Assignment manager"))
                                            {<small>Responsable d'affectation</small>}
                                            @if (User.IsInRole("Intervener"))
                                            {<small>Intervenant</small>}
                                        </p>
                                    </li>
                                    <!-- Menu Body -->
                                    <!-- Menu Footer-->
                                    <li class="user-footer">
                                        <div class="pull-left">
                                            <a href="~/Account/Profil" class="btn btn-default btn-flat">Mon profil</a>
                                        </div>
                                        <div class="pull-right">
                                            @Html.ActionLink("Déconnexion", "SignOut", "Account", null, new { @class = "btn btn-default btn-flat" })
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        }

                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="../../Content/assets3/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>
                            <small>@User.Identity.Name</small>
                        </p>
                        <i class="fa fa-circle text-success"></i>
                        @if (User.IsInRole("Admin"))
                        {<small>Administrateur</small>}
                        @if (User.IsInRole("Reception employee"))
                        {<small>Employé de réception</small>}
                        @if (User.IsInRole("Assignment manager"))
                        {<small>Responsable d'affectation</small>}
                        @if (User.IsInRole("Intervener"))
                        {<small>Intervenant</small>}
                    </div>
                </div>
                <!-- search form -->
                
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">NAVIGATION PRINCIPALE</li>
                    @if (User.IsInRole("Admin"))
                    {
                        <li>
                            <a href="~/Home/Index">
                                <i class="fa fa-dashboard"></i>
                                <span>Tableau de bord</span>
                            </a>
                        </li>
                    }

                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-user"></i> <span>Profil</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Account/Profil"><i class="fa fa-circle-o text-red"></i>Voir le profil</a></li>
                            <li><a href="~/Account/Edit"><i class="fa fa-circle-o text-aqua"></i>Modifier le profil</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="~/Interventions/Index">
                            <i class="fa fa-calendar-check-o "></i>
                            <span>Interventions</span>
                        </a>
                    </li>
                    @if (User.Identity.IsAuthenticated && (User.IsInRole("Admin") || User.IsInRole("Reception employee")))
                    {
                        <li>
                            <a href="~/Requests/Index">
                                <i class="fa fa-book"></i>
                                <span>Demandes</span>
                            </a>
                        </li>
                    }

                    @if (User.Identity.IsAuthenticated && User.IsInRole("Admin"))
                    {
                        <li>
                            <a href="~/Staffs/Index">
                                <i class="fa fa-users"></i>
                                <span>Personnel</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/States/Index">
                                <i class="fa fa-check-square-o"></i>
                                <span>Statuts</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/Agencies/Index">
                                <i class="fa fa-university"></i>
                                <span>Agences</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/Directions/Index">
                                <i class="fa fa-list-ol"></i>
                                <span>Directions</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/Users/Index">
                                <i class="fa fa-users"></i>
                                <span>Utilisateurs</span>
                            </a>
                        </li>
                    }
                    <!--<li class="header">LABELS</li>
    <li><a href="#"><i class="fa fa-circle-o text-red"></i> <span>Important</span></a></li>
    <li><a href="#"><i class="fa fa-circle-o text-yellow"></i> <span>Warning</span></a></li>
    <li><a href="#"><i class="fa fa-circle-o text-aqua"></i> <span>Information</span></a></li> -->
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h2>
                    @ViewBag.Title
                </h2>
            </section>
            <!-- Main content -->
            <section class="content">

                    @RenderBody()
                
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
            </div>
            <strong>Copyright &copy; 2019 <a href="https://www.amendis.ma/fr">Amendis - Veolia</a>.</strong> All rights
            reserved.
        </footer>
        <!-- Control Sidebar -->
        
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="../../Content/assets3/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="../../Content/assets3/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- DataTables -->
    <script src="../../Content/assets3/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="../../Content/assets3/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <!-- SlimScroll -->
    <script src="../../Content/assets3/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="../../Content/assets3/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="../../Content/assets3/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="../../Content/assets3/dist/js/demo.js"></script>
    <!-- page script -->
    <script>
        $(function () {
            $('#example1').DataTable()
            $('#example11').DataTable()
            $('#example111').DataTable()
            $('#example2').DataTable({
                'paging': true,
                'lengthChange': false,
                'searching': false,
                'ordering': true,
                'info': true,
                'autoWidth': false
            })
        })
    </script>
    @RenderSection("scripts", required: false)

</body>
</html>
